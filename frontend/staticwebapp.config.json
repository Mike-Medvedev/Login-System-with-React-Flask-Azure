{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/*.css", "/*.js", "/*.png", "/*.jpg", "/*.svg", "/*.ico", "/static/*", "/assets/*"]
  },
  "routes": [
    {
      "route": "/home",
      "rewrite": "/index.html"
    },
    {
      "route": "/another-route",
      "rewrite": "/index.html"
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ]
}
